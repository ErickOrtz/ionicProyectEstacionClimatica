{
    "builds": [
        {
            "src": "www/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/tabs/app.component.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
